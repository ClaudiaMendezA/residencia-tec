{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "password": {
      "type": "password",
      "required": true,
      "minLength": 6,
      "private": true
    },
    "customer_role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer-role.customer-role",
      "inversedBy": "customers"
    }
  }
}
